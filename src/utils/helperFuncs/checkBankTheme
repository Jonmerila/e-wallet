const checkBankTheme = (bank) => {
  if (bank == "Pinnacle Bank") {
    return "bank-Pinnacle";
  }
  if (bank == "Horizon Financial") {
    return "bank-Hori";
  }
  if (bank == "Silver Oak Bank") {
    return "bank-Silver";
  }
  if (bank == "Bank Name") {
    return "bank-empty";
  }
  return "null-bank";
};

export default checkBankTheme;
